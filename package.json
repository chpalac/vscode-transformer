{
  "name": "transformer",
  "displayName": "transformer",
  "description": "",
  "version": "0.0.2",
  "repository": {
    "url": "https://github.com/chpalac/vscode-transformer"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:transformer.selection",
    "onCommand:transformer.file"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "transformer.file",
        "title": "Make Styles: transform file"
      },
      {
        "command": "transformer.selection",
        "title": "Make Styles: transform selected styles"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@types/prismjs": "^1.26.0",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "ts-loader": "^9.2.8",
    "typescript": "^4.5.5",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@kuscamara/code-sample": "^3.0.2",
    "prismjs": "^1.27.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "v9helper": "^0.0.8",
    "v9helper-transform-style-object": "^0.0.5"
  }
}
